    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^23.6.0",
    "ramda": "^0.27.0",
    "ts-jest": "^23.10.4",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "uglify-js": "^3.4.9",
    "esdoc-typescript-plugin": "^1.0.1"
